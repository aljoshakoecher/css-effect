###  http://www.example.com/simpleDrilling#LongDrillDrilling
:LongDrillDrilling rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty <http://www.w3id.org/plattfom-i40/css-effect#achievesEffect> ;
                                         owl:someValuesFrom :LongDrillDrillingEffect
                                       ] ;
                   rdfs:subClassOf <http://www.w3id.org/hsu-aut/css#Capability> ;
                   rdfs:comment "Capability to drill holes with a long drill up to 200 deep" .


###  http://www.example.com/simpleDrilling#LongDrillDrillingEffect
:LongDrillDrillingEffect rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/plattfom-i40/css-effect#State>
                                                                    [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty :hasHoleDepth ;
                                                                                             owl:hasValue 0
                                                                                           ]
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty :hasHoleDepth ;
                                                                                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                           ]
                                                                                         ) ;
                                                                      rdf:type owl:Class
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.w3id.org/plattfom-i40/css-effect#hasTransitionTo> ;
                                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                      owl:onClass [ owl:intersectionOf ( <http://www.w3id.org/plattfom-i40/css-effect#State>
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :hasHoleDepth ;
                                                                                                           owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                               owl:onDatatype xsd:integer ;
                                                                                                                               owl:withRestrictions ( [ xsd:maxInclusive 200
                                                                                                                                                      ]
                                                                                                                                                    )
                                                                                                                             ]
                                                                                                         ]
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :hasHoleDepth ;
                                                                                                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty <http://www.w3id.org/plattfom-i40/css-effect#hasTransitionTo> ;
                                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                                      owl:onClass <http://www.w3id.org/plattfom-i40/css-effect#State>
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] .


###  http://www.example.com/simpleDrilling#LongDrillMatch
:LongDrillMatch rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :LongDrillDrillingEffect
                                                           :RequiredEffect
                                                         ) ;
                                      rdf:type owl:Class
                                    ] .


###  http://www.example.com/simpleDrilling#RequiredEffect
:RequiredEffect rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( <http://www.w3id.org/plattfom-i40/css-effect#State>
                                                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty :hasHoleDepth ;
                                                                                    owl:hasValue 0
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty :hasHoleDepth ;
                                                                                    owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.w3id.org/plattfom-i40/css-effect#hasTransitionTo> ;
                                                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass [ owl:intersectionOf ( <http://www.w3id.org/plattfom-i40/css-effect#State>
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty :hasHoleDepth ;
                                                                                                  owl:hasValue 120
                                                                                                ]
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty :hasHoleDepth ;
                                                                                                  owl:maxCardinality "1"^^xsd:nonNegativeInteger
                                                                                                ]
                                                                                              ) ;
                                                                           rdf:type owl:Class
                                                                         ]
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty <http://www.w3id.org/plattfom-i40/css-effect#hasTransitionTo> ;
                                                             owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                             owl:onClass <http://www.w3id.org/plattfom-i40/css-effect#State>
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf <http://www.w3id.org/plattfom-i40/css-effect#Effect> ;
                rdfs:comment "This is an example required effect to drill a hole with a depth of 150" .


